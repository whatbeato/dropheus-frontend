{
  "manifest_version": 3,
  "name": "dropheus",
  "description": "find cheaper alternatives or detect dropshipping on etsy, amazon or ebay.",
  "version": "0.1.0",
  "action": {
    "default_title": "Extension Base",
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
